{
  "name": "sdk",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/abi/index.d.ts",
      "default": "./dist/abi/index.js"
    },
    "./wagmi": {
      "types": "./dist/wagmi/index.d.ts",
      "default": "./dist/wagmi/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "default": "./dist/actions/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    }
  },
  "scripts": {
    "generate": "pnpm generate:abi && pnpm generate:wagmi && pnpm generate:actions",
    "generate:abi": "TYPE=abi wagmi generate",
    "generate:wagmi": "TYPE=wagmi wagmi generate",
    "generate:actions": "TYPE=actions wagmi generate",
    "typecheck": "tsc --noEmit",
    "build": "rimraf dist && tsc --removeComments --declaration false && tsc --emitDeclarationOnly",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@superfluid-finance/ethereum-contracts": "^1.12.0",
    "@superfluid-finance/metadata": "^1.5.2",
    "@types/node": "^22.15.3",
    "@wagmi/cli": "^2.3.0",
    "dotenv": "^16.5.0",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "viem": "^2.28.4",
    "vitest": "^3.1.3",
    "wagmi": "^2.15.2"
  },
  "peerDependencies": {
    "@wagmi/core": "^2",
    "react": ">=18",
    "viem": "^2",
    "wagmi": "^2"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    },
    "@wagmi/core": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "viem": {
      "optional": true
    }
  },
  "sideEffects": false
}
